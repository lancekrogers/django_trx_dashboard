{
  "name": "multichain-trx-dashboard",
  "version": "1.0.0",
  "description": "Frontend assets for Multi-Chain Portfolio Dashboard",
  "private": true,
  "scripts": {
    "build-css": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --minify",
    "watch-css": "tailwindcss -i ./static/css/input.css -o ./static/css/output.css --watch",
    "build": "npm run build-css",
    "dev": "npm run watch-css"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "alpinejs": "^3.14.9",
    "chart.js": "^4.5.0",
    "franken-ui": "2.0.0",
    "htmx.org": "^1.9.12",
    "lightweight-charts": "^5.0.7"
  },
  "main": "tailwind.config.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lancekrogers/django_trx_dashboard.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/lancekrogers/django_trx_dashboard/issues"
  },
  "homepage": "https://github.com/lancekrogers/django_trx_dashboard#readme"
}
